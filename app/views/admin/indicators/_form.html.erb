
  <%= semantic_form_for [:admin, @indicator] do |f| %>
    <fieldset class='inputs'>
    <ol>
    <%= f.input :dimension_id, :as => :select, :collection => Dimension.all(:include => [:service_level]).collect{|d| ["#{d.service_level.name} - #{d.name}", d.id]} %>
    <%= f.input :name %>
    <%= f.input :number, :as => :string %>
    
    </ol>
    </fieldset>
    <%= f.buttons :commit %>
  <% end %>

