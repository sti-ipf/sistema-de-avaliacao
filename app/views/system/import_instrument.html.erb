<%= form_tag(import_instrument_path, :id => 'import_instrument_form', :multipart => true) do %>
  <%= label_tag "Informe o arquivo do instrumental a ser importado:" %>
  <%= file_field(:csv_file, 'csv_file', :id => 'csv_file') %>
  <%= submit_tag('Importar', :class => 'submit') %>
<% end %>
<%= link_to "Exemplo de instrumental", "/template/instrumental_de_exemplo.csv" %>