<%= form_tag(import_schools_path, :id => 'import_schools_form', :multipart => true) do %>
  <%= label_tag "Informe o arquivo das escolas a ser importado:" %>
  <%= file_field(:csv_file, 'csv_file', :id => 'csv_file') %>
  <%= submit_tag('Importar', :class => 'submit') %>
<% end %>
<%= link_to "Exemplo de arquivo", "/template/escolas_de_exemplo.csv" %>